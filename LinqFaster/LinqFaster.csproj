<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.3;net461</TargetFrameworks>
    <PackageId>LinqFaster</PackageId>
    <PackageVersion>0.9.1</PackageVersion>
    <Title>LinqFaster</Title>
    <Authors>Jack Mott</Authors>
    <Description>High performance Linq-like extension methods for Arrays and Lists</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Initial Beta</PackageReleaseNotes>
    <Copyright>Copyright 2017 (c) Jack Mott. All rights reserved.</Copyright>
    <PackageTags>Linq Performance</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>0.9.0</Version>
    <PackageProjectUrl>https://github.com/jackmott/LinqFaster</PackageProjectUrl>
    <RepositoryUrl>https://github.com/jackmott/LinqFaster</RepositoryUrl>
    <IconUrl>https://jackmott.github.io/images/LinqFaster.png</IconUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageIconUrl>https://jackmott.github.io/images/LinqFaster.png</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Update="LinqFaster.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>